<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoardInvitationMailSender.Domain"
      assembly="SocioBoardInvitationMailSender">

  <class name="Mantaemail" table="Mantaemail" lazy="false">
    <id name="Id" column="Id">
      <generator class="identity"/>
    </id>
    <property name="Name">
      <column name="Name" />
    </property>
    <property name="Email">
      <column name="Email" />
    </property>
    <property name="Status">
      <column name="Status" />
    </property>
    <property name="Bounce">
      <column name="Bounce" />
    </property>
    <property name="Spam">
      <column name="Spam" />
    </property>
    <property name="IsInvalid">
      <column name="IsInvalid" />
    </property>
  </class>
</hibernate-mapping> 
