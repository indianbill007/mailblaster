<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="LinkedInMessage" table="LinkedInMessage" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="Message">
      <column name="Message" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate"  />
    </property>
    <property name="EntryDate">
      <column name="EntryDate"  />
    </property>
    <property name="ProfileId">
      <column name="ProfileId"  />
    </property>


    <property name="FromId">
      <column name="FromId"  />
    </property>

    <property name="FromName">
      <column name="FromName"  />
    </property>

    <property name="FromUrl">
      <column name="FromUrl"  />
    </property>

   

    
    <property name="Type">
      <column name="Type"  />
    </property>

    <property name="UserId">
      <column name="UserId"  />
    </property>

  </class>
</hibernate-mapping>