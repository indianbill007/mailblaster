<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoard.Domain"
      assembly="SocioBoard">

  <class name="Coupon" table="Coupon" lazy="false">
    <id name="Id" column="Id">
      <generator class="guid"/>
    </id>
    <property name="CouponCode">
      <column name="CouponCode" />
    </property>
    <property name="EntryCouponDate">
      <column name="EntryCouponDate" />
    </property>
    <property name="ExpCouponDate">
      <column name="ExpCouponDate"  />
    </property>
    <property name="Status">
      <column name="Status"  />
    </property>

  </class>
</hibernate-mapping>
