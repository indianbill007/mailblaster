<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="SocioBoardInvitationMailSender.Domain"
      assembly="SocioBoardInvitationMailSender">

  <class name="Mandrillaccount" table="Mandrillaccount" lazy="false">
    <id name="Id" column="Id">
      <generator class="identity"/>
    </id>
    <property name="UserName">
      <column name="UserName" />
    </property>
    <property name="Password">
      <column name="Password" />
    </property>
    <property name="Total">
      <column name="Total" />
    </property>
    <property name="Status">
      <column name="Status" />
    </property>
    <property name="Sent">
      <column name="Sent" />
    </property>
    <property name="Opens">
      <column name="Opens" />
    </property>
    <property name="Clicks">
      <column name="Clicks" />
    </property>
    <property name="EntryDate">
      <column name="EntryDate" />
    </property>
  </class>
</hibernate-mapping>
